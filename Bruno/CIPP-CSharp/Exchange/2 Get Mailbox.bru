meta {
  name: Get Mailbox
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/v1/exchange/mailboxes/{{mailboxId}}?tenantId={{exampleTenant}}
  body: none
  auth: inherit
}

params:path {
  mailboxId: {{mailboxId}}
}

params:query {
  tenantId: {{exampleTenant}}
}

headers {
  Accept: application/json
  Content-Type: application/json
}

settings {
  encodeUrl: true
}

meta {
  name: Create Application Credential
  type: http
  seq: 5
}

post {
  url: {{baseUrl}}/v1/applications/applications/{{applicationId}}/credentials?tenantId={{exampleTenant}}
  body: json
  auth: inherit
}

params:query {
  tenantId: {{exampleTenant}}
}

headers {
  Accept: application/json
  Content-Type: application/json
}

body:json {
  {
    "type": "Password",
    "displayName": "New Credential",
    "customKeyIdentifier": null
  }
}

settings {
  encodeUrl: true
}
